{
  "name": "@mmomtchev/react-native-settings",
  "version": "1.0.1",
  "description": "React Native Universal Settings Screen",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:lib && npm run build:examples && npm run build:doc",
    "build:lib": "tsc",
    "build:examples": "webpack --mode=production",
    "build:doc": "documentation readme src/*.tsx --parse-extension=tsx --require-extension=tsx --section=API",
    "publish:examples": "node publish-ghpages.cjs",
    "clean": "tsc --build --clean && rimraf docs",
    "eslint": "eslint src/*.ts* examples/*.ts*",
    "test": "jest",
    "dev": "webpack-cli serve --mode=development --env development --open",
    "codecov": "curl -s https://codecov.io/bash | bash",
    "preversion": "npm run test",
    "version": "npm run clean && npm run build && git add package.json README.md",
    "postversion": "git push && git push --tags && npm run publish:examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmomtchev/react-native-settings.git"
  },
  "keywords": [
    "React",
    "react-native",
    "settings",
    "preferences"
  ],
  "author": "Momtchil Momtchev <momtchil@momtchev.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmomtchev/react-native-settings/issues"
  },
  "homepage": "https://github.com/mmomtchev/react-native-settings#readme",
  "peerDependencies": {
    "@react-navigation/native": ">=6.0.0",
    "@react-navigation/native-stack": ">=6.6.0",
    "@types/react": ">=17.0.0",
    "react": ">=17.0.0",
    "react-native": ">=0.68.0"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^1.17.4",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^18.0.4",
    "@types/react-native": "^0.67.7",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-native-web": "^0.17.7",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.7.1",
    "documentation": "^13.2.5",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "expo-dev-menu": "^0.10.6",
    "gh-pages": "^4.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-haste-map": "^27.5.1",
    "markdown-loader": "^8.0.0",
    "prettier": "^2.6.2",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.28.0",
    "raw-loader": "^4.0.2",
